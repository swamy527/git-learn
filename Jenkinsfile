    stages {
        stage('Example') {
            steps {
                echo 'Hello World'
            }
        }
         stage('second-stage') {
            steps {
                echo 'Hello babai World'
            }
        }
         stage('third-stage') {
            steps {
                echo 'Hello das World'
            }
        }
    }
}